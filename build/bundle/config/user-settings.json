{
    "DefaultProfile": "PowerShell7",
    "StartupDir": null,
    "WindowsTerminalPath": "%CMDLINE%\\terminal2\\wt.exe",
    "CustomProfiles": {
        "PowerShell7": {
            "ShellPath": "%PS%\\pwsh.exe",
            "Arguments": "-NoLogo",
            "Title": "PowerShell 7"
        },
        "PowerShellPreview": {
            "ShellPath": "%PS%\\pwsh.exe",
            "Arguments": "-NoLogo",
            "Title": "PowerShell Preview"
        },
        "CustomPwsh": {
            "ShellPath": "%PS%\\pwsh.exe",
            "Arguments": "-NoLogo -NoExit -Command \"Write-Host 'Custom PowerShell!' -ForegroundColor Cyan\"",
            "Title": "My Custom PowerShell"
        }
    }
}