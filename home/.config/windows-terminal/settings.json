    "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",

    "copyOnSelect": false,
    "copyFormatting": "none",

    "profiles": {
        "defaults": {
            "font": {
                "face": "Cascadia Code",
                "size": 11,
                "weight": "normal"
            },
            "cursorShape": "bar",
            "cursorColor": "#FFFFFF",
            "colorScheme": "Naner Dark",
            "opacity": 95,
            "useAcrylic": false,
            "padding": "8, 8, 8, 8",
            "antialiasingMode": "grayscale"
        },
        "list": [
            {
                "guid": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
                "name": "PowerShell (Naner)",
                "commandline": "%NANER_ROOT%\\vendor\\powershell\\pwsh.exe -NoLogo",
                "icon": "%NANER_ROOT%\\icons\\powershell.png",
                "startingDirectory": "%NANER_ROOT%",
                "colorScheme": "Naner Dark",
                "tabTitle": "PowerShell"
            },
            {
                "guid": "{b453ae62-4e3d-5e58-b989-0a998ec441b8}",
                "name": "Bash (Naner)",
                "commandline": "%NANER_ROOT%\\vendor\\msys64\\usr\\bin\\bash.exe --login -i",
                "icon": "%NANER_ROOT%\\icons\\bash.png",
                "startingDirectory": "%NANER_ROOT%",
                "colorScheme": "Naner Dark",
                "tabTitle": "Bash"
            },
            {
                "guid": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
                "name": "CMD (Naner)",
                "commandline": "cmd.exe /K %NANER_ROOT%\\bin\\naner-launcher.bat cmd",
                "icon": "%NANER_ROOT%\\icons\\cmd.png",
                "startingDirectory": "%NANER_ROOT%",
                "colorScheme": "Naner Dark",
                "tabTitle": "Command Prompt"
            },
            {
                "guid": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
                "name": "PowerShell",
                "commandline": "powershell.exe",
                "hidden": false,
                "colorScheme": "Campbell"
            },
            {
                "guid": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
                "name": "Command Prompt",
                "commandline": "cmd.exe",
                "hidden": false,
                "colorScheme": "Campbell"
            }
        ]
    },

